{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@nx/js": "21.1.3",
    "@nx/node": "21.1.3",
    "@nx/webpack": "21.1.3",
    "@types/express": "4.17.23",
    "@types/node": "20.19.0",
    "express": "^4.21.2",
    "tslib": "2.3.0",
    "typescript": "5.7.2",
    "webpack-cli": "5.1.4"
  },
  "nx": {
    "targets": {
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "backend:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "backend:build:development"
          },
          "production": {
            "buildTarget": "backend:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "main": "main.js"
}