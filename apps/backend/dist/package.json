{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@nx/js": "21.1.3",
    "@nx/node": "21.1.3",
    "@nx/webpack": "21.1.3",
    "@types/express": "4.17.23",
    "async": "3.2.6",
    "bcryptjs": "^2.4.3",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "4.4.1",
    "depd": "2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "is-arrayish": "0.2.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "ms": "2.1.3",
    "on-finished": "2.4.1",
    "on-headers": "1.0.2",
    "pg": "^8.11.3",
    "readable-stream": "3.6.2",
    "redis": "^4.6.10",
    "safe-buffer": "5.2.1",
    "semver": "7.7.2",
    "tslib": "2.8.1",
    "typescript": "5.7.2",
    "uuid": "^9.0.1",
    "vary": "1.1.2",
    "webpack-cli": "5.1.4",
    "winston": "^3.11.0",
    "yallist": "3.1.1"
  },
  "nx": {
    "targets": {
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "backend:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "backend:build:development"
          },
          "production": {
            "buildTarget": "backend:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "main": "main.js"
}